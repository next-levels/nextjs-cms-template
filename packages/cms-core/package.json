{
  "name": "@mikestraczek/cms-core",
  "version": "1.0.0",
  "description": "Core utilities and types for the CMS template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cms",
    "nextjs",
    "typescript",
    "utilities"
  ],
  "author": "Mike Straczek",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "eslint": "^9.31.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "date-fns": "^4.1.0"
  }
}
