  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.31.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {
    "@mikestraczek/cms-core": "^1.0.1",
    "@radix-ui/react-avatar": "^1.1.10",
