  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cms",
    "ui",
    "components",
    "react",
    "tailwind"
  ],
  "author": "Mike Straczek",
