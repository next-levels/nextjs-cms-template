    "date-fns": "^4.1.0",
    "tailwind-merge": "^2.6.0",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.9",
    "next": "^15.4.3",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.8.0",
    "react-hook-form": "^7.61.0",
    "sonner": "^2.0.6"
